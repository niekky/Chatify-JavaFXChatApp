<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.RoomController">
   <children>
      <ListView fx:id="roomsList" layoutX="134.0" layoutY="78.0" prefHeight="200.0" prefWidth="332.0" />
      <Label layoutX="223.0" layoutY="39.0" text="All rooms available">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="text_field" layoutX="168.0" layoutY="332.0" />
      <Button fx:id="createRoomButton" layoutX="334.0" layoutY="332.0" mnemonicParsing="false" onAction="#createRoom" prefHeight="25.0" prefWidth="98.0" text="Create Room" />
      <Button fx:id="accountSettingButton" layoutX="492.0" layoutY="14.0" mnemonicParsing="false" onAction="#navigateToAccountSetting" prefWidth="101.0" text="Account Setting" />
      <Button fx:id="joinRoomButton" layoutX="281.0" layoutY="287.0" mnemonicParsing="false" onAction="#joinRoom" text="Join" />
   </children>
</AnchorPane>
