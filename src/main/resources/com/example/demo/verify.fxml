<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.VerifyController">
   <children>
      <TextField fx:id="verifyUsernameTextField" layoutX="226.0" layoutY="175.0" />
      <TextField fx:id="verifyPasswordTextField" layoutX="226.0" layoutY="213.0" />
      <Label layoutX="163.0" layoutY="179.0" text="Username" />
      <Label layoutX="165.0" layoutY="217.0" text="Password" />
      <Button fx:id="verifyButton" layoutX="277.0" layoutY="261.0" mnemonicParsing="false" onAction="#verify" text="Verify" />
      <Label layoutX="156.0" layoutY="85.0" text="Please Verify your Account!">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="warning" contentDisplay="CENTER" layoutX="226.0" layoutY="238.0" prefHeight="15.0" prefWidth="149.0" textFill="RED">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
