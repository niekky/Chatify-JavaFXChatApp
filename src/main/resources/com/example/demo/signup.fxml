<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.AccController">
   <children>
      <TextField fx:id="signupUsernameTextField" layoutX="237.0" layoutY="136.0" />
      <TextField fx:id="signupPasswordTextField" layoutX="237.0" layoutY="170.0" />
      <Button layoutX="234.0" layoutY="302.0" mnemonicParsing="false" onAction="#navigateToLogin" prefHeight="25.0" prefWidth="156.0" text="Go Back" />
      <Button fx:id="signupButton" layoutX="234.0" layoutY="264.0" mnemonicParsing="false" onAction="#signup" prefHeight="25.0" prefWidth="156.0" text="Sign up" />
      <Label layoutX="171.0" layoutY="140.0" text="Username" />
      <Label layoutX="173.0" layoutY="174.0" text="Password" />
      <TextField fx:id="signupConPasswordTextField" layoutX="237.0" layoutY="206.0" />
      <Label layoutX="129.0" layoutY="210.0" text="Confirm Password" />
      <Label fx:id="warningSignup" contentDisplay="CENTER" layoutX="237.0" layoutY="239.0" prefHeight="15.0" prefWidth="149.0" textFill="RED">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
