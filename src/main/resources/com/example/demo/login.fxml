<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.AccController">
   <children>
      <Label alignment="CENTER" layoutY="83.0" prefHeight="43.0" prefWidth="600.0" text="Chatify" AnchorPane.topAnchor="83.0">
         <font>
            <Font name="Candara Light" size="48.0" />
         </font>
      </Label>
      <TextField fx:id="loginUsernameTextField" layoutX="226.0" layoutY="213.0" />
      <TextField fx:id="loginPasswordTextField" layoutX="226.0" layoutY="247.0" />
      <Button fx:id="loginButton" layoutX="222.0" layoutY="287.0" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="156.0" text="Login" />
      <Button layoutX="223.0" layoutY="318.0" mnemonicParsing="false" onAction="#navigateToSignup" prefHeight="25.0" prefWidth="156.0" text="Sign up" />
      <Label layoutX="160.0" layoutY="217.0" text="Username" />
      <Label layoutX="162.0" layoutY="251.0" text="Password" />
      <Label fx:id="warning" contentDisplay="CENTER" layoutX="225.0" layoutY="272.0" prefHeight="15.0" prefWidth="149.0" textFill="RED">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
