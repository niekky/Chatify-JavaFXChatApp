<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ConversationController">
   <children>
      <ListView fx:id="conversationList" layoutX="45.0" layoutY="73.0" prefHeight="230.0" prefWidth="345.0" />
      <TextField fx:id="text_field" layoutX="45.0" layoutY="315.0" prefHeight="25.0" prefWidth="345.0" />
      <Button fx:id="sendButton" layoutX="404.0" layoutY="315.0" mnemonicParsing="false" onAction="#sendChat" prefHeight="25.0" prefWidth="74.0" text="Send" />
      <ListView fx:id="membersList" layoutX="423.0" layoutY="71.0" prefHeight="200.0" prefWidth="140.0" />
      <Label layoutX="456.0" layoutY="39.0" text="Members">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="25.0" layoutY="353.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <Label fx:id="chatroomName" layoutX="46.0" layoutY="22.0" text="Chatroom">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
